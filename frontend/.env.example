# United Voice Agent - Frontend Environment Variables
# Copy this to .env.local and configure for your environment

# Backend URLs (adjust ports if needed)
NEXT_PUBLIC_BACKEND_HTTP_URL=http://localhost:8000
NEXT_PUBLIC_BACKEND_WEBSOCKET_URL=http://localhost:8000
NEXT_PUBLIC_SOCKET_URL=http://localhost:8000

# API Configuration
NEXT_PUBLIC_API_BASE_URL=http://localhost:8000

# Development settings
NEXT_PUBLIC_ENV=development
NEXT_PUBLIC_DEBUG=true

# Feature flags
NEXT_PUBLIC_ENABLE_WEBSOCKET=true
NEXT_PUBLIC_ENABLE_VOICE_RECORDING=true
NEXT_PUBLIC_ENABLE_REAL_TIME_TRANSCRIPTION=true

# Production overrides (uncomment for production)
# NEXT_PUBLIC_BACKEND_HTTP_URL=https://your-backend.com
# NEXT_PUBLIC_BACKEND_WEBSOCKET_URL=https://your-backend.com
# NEXT_PUBLIC_ENV=production
# NEXT_PUBLIC_DEBUG=false